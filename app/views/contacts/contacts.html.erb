<h1>Contact</h1>

<%= form_for :contact, url: contacts_path do |f| %>
  <p>
    <%= f.label :title %> <br />
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :name %>  <br />
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :email %> <br />
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :message %>  <br />
    <%= f.text_area :message %>
  </p>

  <p>
    <%= f.submit :title %>
  </p>

<% end %>